{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "tokyo-smoke": "#59636e",
    "tokyo-foreground": "#eff2f5",
    "tokyo-blue": "#0969da",
    "tokyo-green": "#009b2c",
    "tokyo-white": "#ffffff",
    "tokyo-rose": "#cf222e",
    "tokyo-orange": "#ff8945",
    "tokyo-teal": "#0fcefd",
    "tokyo-purple": "#8250df"
  },
  "transient_prompt": {
    "template": "\uf105 ",
    "background": "transparent",
    "foreground": "p:tokyo-purple"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}<b>{{.UserName}}@{{.HostName}}</b> \udb80\udd3e",
          "foreground": "p:tokyo-purple",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "home_icon": "\uf015 ",
            "folder_icon": "..",
            "folder_separator_template": " \ueab6 ",
            "style": "agnoster_short",
            "max_depth": 3
          },
          "template": " {{ .Path }} \udb80\udd3e",
          "foreground": "p:tokyo-smoke",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:tokyo-blue",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:tokyo-green{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:tokyo-orange{{ end }}",
            "{{ if gt .Ahead 0 }}p:tokyo-teal{{ end }}",
            "{{ if gt .Behind 0 }}p:tokyo-rose{{ end }}"
          ],
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": " \uf0e7 ",
          "foreground_templates": [
            "{{ if eq .Code 0 }}p:tokyo-green{{ end }}",
            "{{ if gt .Code 0 }}p:tokyo-rose{{ end }}"
          ],
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:tokyo-purple",
          "style": "plain",
          "type": "text",
          "template": "\uf105"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}
